      "host": "%env(string:DB_HOST)%",
      "port": "%env(string:DB_PORT)%",
      "user": "%env(string:DB_USER)%",
      "#password": "%env(string:DB_PASSWORD)%",
      "database": "%env(string:DB_DATABASE)%",
      "ssh": {
        "enabled": true,
        "keys": {
          "#private": "%env(string:SSH_PRIVATE_KEY)%",
          "public": "%env(string:SSH_PUBLIC_KEY)%"
